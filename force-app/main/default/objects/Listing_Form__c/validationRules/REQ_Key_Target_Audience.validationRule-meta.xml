<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>REQ_Key_Target_Audience</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status__c ,&apos;Submitted&apos;),
ISBLANK( Key_Target_Audience__c )
)</errorConditionFormula>
    <errorDisplayField>Key_Target_Audience__c</errorDisplayField>
    <errorMessage>Key Target Audience is required for submission</errorMessage>
</ValidationRule>
