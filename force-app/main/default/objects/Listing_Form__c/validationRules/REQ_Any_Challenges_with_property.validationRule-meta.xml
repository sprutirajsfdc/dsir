<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>REQ_Any_Challenges_with_property</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL( Status__c ,&apos;Submitted&apos;),
ISBLANK( Any_Challenges_with_property__c )
)</errorConditionFormula>
    <errorDisplayField>Any_Challenges_with_property__c</errorDisplayField>
    <errorMessage>Any Challenges with property is required for submission</errorMessage>
</ValidationRule>
