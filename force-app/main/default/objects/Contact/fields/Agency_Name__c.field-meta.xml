<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Agency_Name__c</fullName>
    <description>If Company record type is 3rd party agency then show name with hyperlink.</description>
    <externalId>false</externalId>
    <formula>IF(Account.RecordType.Name = &quot;SystemInternal&quot; , &apos;Individual&apos;  , 
  HYPERLINK(&quot;/&quot;&amp;Account.Id, Account.Name ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Agency Name</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
