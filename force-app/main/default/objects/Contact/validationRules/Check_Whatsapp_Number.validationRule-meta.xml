<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Whatsapp_Number</fullName>
    <active>false</active>
    <errorConditionFormula>IF( ISPICKVAL( Is_Mobile_Whatsapp_Number__c , &quot;NO&quot;)  &amp;&amp;   ISBLANK( WhatsApp_Number__c ) , True, False)</errorConditionFormula>
    <errorDisplayField>AccountId</errorDisplayField>
    <errorMessage>Please fill the WhatsApp number</errorMessage>
</ValidationRule>
