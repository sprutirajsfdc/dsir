<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Only_Call_Center_can_set_these_status</fullName>
    <active>false</active>
    <errorConditionFormula>AND
(
(ISPICKVAL(  os_Stage__c , &quot;Closing&quot;)),
(ISPICKVAL( os_Stage__c , &quot;Expired&quot;)),
(ISPICKVAL( os_Stage__c , &quot;Delete&quot;)),
$UserRole.Id = &quot;00EHp000000bTs3MAE&quot;
)</errorConditionFormula>
    <errorMessage>You are not allowed to set these statues. Please refer to your manager.</errorMessage>
</ValidationRule>
