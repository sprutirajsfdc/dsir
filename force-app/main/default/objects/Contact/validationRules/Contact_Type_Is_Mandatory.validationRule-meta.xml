<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_Type_Is_Mandatory</fullName>
    <active>false</active>
    <errorConditionFormula>AND( ISNEW() ,(NOT(ISPICKVAL(channel_source__c,&quot;Website Form&quot; ))) , ( ISPICKVAL(ContactType__c,&apos;&apos;)))</errorConditionFormula>
    <errorDisplayField>ContactType__c</errorDisplayField>
    <errorMessage>Contact Type Is required</errorMessage>
</ValidationRule>
