<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>IF_Agency_Trade_License_is_required</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
ISPICKVAL( ContactType__c , &quot;Agency&quot;),
ISBLANK ( Agency_Trade_License_No__c )
)</errorConditionFormula>
    <errorDisplayField>Agency_Trade_License_No__c</errorDisplayField>
    <errorMessage>REQUIRED: Trade License No.</errorMessage>
</ValidationRule>
