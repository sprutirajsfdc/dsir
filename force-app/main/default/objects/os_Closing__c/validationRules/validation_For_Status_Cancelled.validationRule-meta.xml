<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>validation_For_Status_Cancelled</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISBLANK(TEXT( os_Reason_for_Cancelled__c )),
ISPICKVAL(os_Closing_Status__c, &apos;Canceled&apos;))</errorConditionFormula>
    <errorMessage>Please fill this field - Reason for Cancelled</errorMessage>
</ValidationRule>
