<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>No_Listing_Blocks_Path</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISBLANK(os_Listing__c),
OR
(
(ISPICKVAL(os_Closing_Status__c ,&quot;Contract Approval&quot;)),
(ISPICKVAL(os_Closing_Status__c ,&quot;In Transfer&quot;)),
(ISPICKVAL(os_Closing_Status__c ,&quot;Compliance&quot;)),
(ISPICKVAL(os_Closing_Status__c ,&quot;Rented&quot;)),
(ISPICKVAL(os_Closing_Status__c ,&quot;Closed&quot;)),
(ISPICKVAL(os_Closing_Status__c ,&quot;Cancelled&quot;))

))</errorConditionFormula>
    <errorDisplayField>os_Listing__c</errorDisplayField>
    <errorMessage>Please fill &quot;Listings&quot; field.</errorMessage>
</ValidationRule>
