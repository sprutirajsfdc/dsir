<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fill_Mobile_or_Email</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 ISBLANK( Mobile__c ),
 ISBLANK( E_MAIL__c )  
 )</errorConditionFormula>
    <errorMessage>Please Fill Mobile Or Email.</errorMessage>
</ValidationRule>
