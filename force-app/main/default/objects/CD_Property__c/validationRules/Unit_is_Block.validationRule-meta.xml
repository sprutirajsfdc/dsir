<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Unit_is_Block</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
ISPICKVAL(Status__c , &quot;Block&quot;),
ISPICKVAL(Reason_for_Block__c  ,&quot;&quot; )
)</errorConditionFormula>
    <errorDisplayField>Reason_for_Block__c</errorDisplayField>
    <errorMessage>Please select the Reason for Block</errorMessage>
</ValidationRule>
