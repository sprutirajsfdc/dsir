<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Company_must_be_selected</fullName>
    <active>true</active>
    <description>This is mandatory as it trigger the initials for the brokers reference number</description>
    <errorConditionFormula>ISPICKVAL( gsir_company__c , &quot;&quot;)</errorConditionFormula>
    <errorDisplayField>gsir_company__c</errorDisplayField>
    <errorMessage>Company cannot be empty</errorMessage>
</ValidationRule>
