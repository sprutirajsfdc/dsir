<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>If_OC_Deposit_Is_Others</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL(OC_Deposit__c , &quot;Others&quot;),OR(ISBLANK( Other_OC_Notes__c )))</errorConditionFormula>
    <errorMessage>Please add Other OC Notes</errorMessage>
</ValidationRule>
