    <formulas>
        <name>Closing_Formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a08&quot;,{!recordId} ,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>Conveyance_Formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a2N&quot;,{!recordId} ,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>Currencyformula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a0Q&quot;,TEXT({!Getting_Inquiry.CurrencyIsoCode}) , 
IF(LEFT({!recordId}, 3) == &quot;a2N&quot;, TEXT({!Getting_Conveyance.CurrencyIsoCode}),
 IF(LEFT({!recordId}, 3) == &quot;a0E&quot;,  TEXT({!Getting_Listing.CurrencyIsoCode}) ,
IF(LEFT({!recordId}, 3) == &quot;a08&quot;, TEXT({!Getting_Closing.CurrencyIsoCode}),
IF(LEFT({!recordId}, 3) == &quot;a0G&quot;, TEXT({!Getting_Offer.CurrencyIsoCode}),
IF(LEFT({!recordId}, 3) == &quot;a13&quot;, TEXT({!Getting_Property_valuation.CurrencyIsoCode}),
&apos;&apos; ))
))))</expression>
    </formulas>
    <formulas>
        <name>FileUploadCheck</name>
        <dataType>Boolean</dataType>
        <expression>{!VarContentDocumentIds}=&apos;[]&apos;</expression>
    </formulas>
    <formulas>
        <name>Inquiry_Formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a0Q&quot;,{!recordId} ,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>Listing_Formula</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a0E&quot;,{!recordId} ,&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>PrimaryContact</name>
        <dataType>String</dataType>
        <expression>IF(LEFT({!recordId}, 3) == &quot;a0Q&quot; &amp;&amp; {!Getting_Inquiry.RecordTypeId} != &apos;0127T000000Gr2oQAC&apos;, {!Getting_Inquiry.pba__Contact__c} , IF(LEFT({!recordId}, 3) == &quot;a0E&quot;, {!Getting_Listing.PropertyOwnerContact__c}, 
IF(LEFT({!recordId}, 3) == &quot;a3B&quot;,{!Getting_Property_valuation.Contact__c},
IF(LEFT({!recordId}, 3) == &quot;003&quot; ,{!recordId},&apos;&apos; ) 
)))</expression>
    </formulas>
    <formulas>
        <name>PropertyValuationFormula</name>
        <dataType>String</dataType>
        <expression> IF(LEFT({!recordId}, 3) ==  &apos;a3B&apos; ,{!recordId},&apos;&apos;)</expression>
    </formulas>
    <formulas>
        <name>Status_Formula</name>
        <dataType>String</dataType>
        <expression>IF(
    LEFT({!recordId}, 3) == &quot;a0Q&quot;,
    IF(
        {!Getting_Inquiry.RecordTypeId} =&apos;0127T000000Gr2oQAC&apos;,
        &quot;Draft&quot;,
        &quot;Submitted&quot;
    ),
    IF(
        LEFT({!recordId}, 3) == &quot;a2N&quot;,
        &quot;Submitted&quot;,
    IF(
        LEFT({!recordId}, 3) == &quot;a3B&quot;,
        &quot;Submitted&quot;,
    IF(
        LEFT({!recordId}, 3) == &quot;a0E&quot;,
        &quot;Submitted&quot;,
        &quot;Draft&quot;
)
    )

    )
)</expression>
    </formulas>
